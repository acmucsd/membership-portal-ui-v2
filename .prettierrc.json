{
  "quoteProps": "as-needed",
  "singleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "proseWrap": "always",
  "bracketSpacing": true,
  "trailingComma": "es5",
  "semi": true,
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "endOfLine": "auto"
}
